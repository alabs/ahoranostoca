<section id="processes" class="wrapper-home home-section bg-wall section-center section-padding-8">
  <div class="row" id="highlighted-processes">
    <div class="columns small-centered large-10 text-center mb-3">
      <h2 class="text-uppercase" style="font-size:2.2rem;line-height:1.1">¿Qué puedes hacer en Ahora Nos Toca Participar?</h2>
    </div>
    <div class="row collapse">
      <div class="row small-up-1 smallmedium-up-2 mediumlarge-up-3
        large-up-4 card-grid">
        <% highlighted_processes.each do |process| %>
          <div class="column">
            <article class="card card--process card--mini">
              <%= link_to decidim_participatory_processes.participatory_process_path(process), class: "card__link" do %>
                <span class="show-for-sr"><%= translated_attribute process.title %></span>
                <div class="card__image-top"
                  style="background-image:url(<%= process.hero_image.url %>)"></div>
              <% end %>
              <div class="card__content">
                <%= link_to decidim_participatory_processes.participatory_process_path(process), class: "card__link" do %>
                  <h5 class="card__title"><%= decidim_html_escape(translated_attribute(process.title)) %></h5>
                <% end %>
              </div>
            </article>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</section>
